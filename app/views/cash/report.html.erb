<div class="label">
	<span>Caixas Fechados</span>
</div>

<div>
	<%= form_tag cash_filter_date_path, method: :get do %>
		<div class="nav-right">
			<%= date_field_tag :begin_date, @begin_date %>
			<%= date_field_tag :end_date, @end_date %>
			<%= button_to 'Filtrar', nil, class: 'btn btn-success' %>
		</div>	
	<% end %>	
	<%= render 'cashes' %>
</div>